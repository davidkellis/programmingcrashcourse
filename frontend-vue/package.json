{
  "name": "frontend-vue",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --fix",
    "format": "prettier --write src/",
    "deploy:build": "npm run build",
    "deploy:serve": "npm run preview",
    "deploy:full": "npm run deploy:build && npm run deploy:serve"
  },
  "dependencies": {
    "@ruby/3.2-wasm-wasi": "^2.7.1",
    "@ruby/wasm-wasi": "^2.7.1",
    "markdown-it": "^14.1.0",
    "mathjs": "^14.6.0",
    "pinia": "^3.0.3",
    "prismjs": "^1.30.0",
    "vue": "^3.5.19",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.2",
    "@types/node": "^24.3.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/tsconfig": "^0.8.1",
    "eslint": "^9.33.0",
    "eslint-plugin-vue": "~10.4.0",
    "jiti": "^2.5.1",
    "npm-run-all2": "^8.0.4",
    "prettier": "3.6.2",
    "terser": "^5.43.1",
